{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureSpeech": {
    "SubscriptionKey": "",
    "Region": "",
    "Language": "en-US",
    "MaxQueuedFrames": 256,
    "SampleRate": 16000,
    "BitsPerSample": 16,
    "Channels": 1,
    "EnableDetailedOutput": false
  },
  "ConversationContext": {
    "SlidingWindow": "00:05:00",
    "MaxWindowCharacters": 4000,
    "MaxAskedRecently": 50,
    "MaxKnownFacts": 64
  },
  "RollingSummary": {
    "MaxSummaryCharacters": 800
  },
  "LlmService": {
    "BaseUrl": "",
    "ApiKey": "",
    "MaxCandidates": 20,
    "MaxRetries": 3,
    "Timeout": "00:00:12"
  },
  "QuestionGeneration": {
    "FallbackQuestionCount": 5
  },
  "PiiRedaction": {
    "Enabled": false
  },
  "Throttling": {
    "MaxAudioFramesPerSecond": 50,
    "MaxAudioPayloadBytes": 4096,
    "GenerateQuestionsCooldown": "00:00:05"
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  }
}
